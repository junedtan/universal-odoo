<openerp>
	<data>
		
		<!-- REGION/HOMEBASE -->
		
		<record model="ir.ui.view" id="chjs_region_form">
			<field name="name">chjs.region.form</field>
			<field name="model">chjs.region</field>
			<field name="inherit_id" ref="chjs_region.chjs_region_form" />
