	<!-- CHANGE PASSWORD -->

	<t t-name="website_mobile_app_change_password">
		<div class="full_height container universal_website_mobile_app">
			<div class="col-xs-12">
				<div class="form-group ml10">
					<label for="change_password_old">Old Password</label>
					<input type="password" id="change_password_old" class="form-control"/>
				</div>
				<div class="form-group ml10">
					<label for="change_password_new">New Password</label>
					<input type="password" id="change_password_new" class="form-control"/>
				</div>
				<div class="form-group ml10">
					<label for="change_password_retype">Retype New Password</label>
					<input type="password" id="change_password_retype" class="form-control"/>
				</div>
			</div>
			<div class="row ml10">
				<div class="col-md-1 form-group">
					<button type="button" id="btn_change_password" class="btn btn-success">Change Password</button>
				</div>
			</div>
		</div>
	</t>


	<!-- LIST SHUTTLE -->

	<t t-name="website_mobile_app_list_shuttle">
		<t class="full_height container universal_website_mobile_app">
			<h2>Select Contract:</h2>
			<div class="form-group ml10">
				<label for="list_shuttle_contract">Contract</label>
				<select id="list_shuttle_contract" class="form-control">
					<t t-foreach="contract_datas" t-as="contract_data">
						<t t-if="contract_data.service_type == 'Shuttle'">
							<option t-attf-value="{{contract_data.id}}"><t t-esc="contract_data.name"/></option>
						</t>
					</t>
				</select>
			</div>
		</t>
		<div id="website_mobile_app_list_shuttle_schedules"></div>
	</t>

	<t t-name="website_mobile_app_list_shuttle_schedules">
		<t t-foreach="days" t-as="day">
			<div class="row_hoverable accordion">
				<h5><t t-esc="day"/></h5>
			</div>
			<t t-if="shuttle_datas[day_value].length == 0">
				<div class="detail_container accordion_detail" >
					<div class="col-xs-12" style="text-align: center; color: darkgray;">
						<h4>Data Kosong</h4>
					</div>
				</div>
			</t>
			<t t-if="shuttle_datas[day_value].length != 0">
				<div class="detail_container accordion_detail" >
					<t t-foreach="shuttle_datas[day_value]" t-as="shuttle">
						<div class="list_title">
							<b><t t-esc="shuttle.name"/></b>
						</div>
						<table class="table">
							<tr>
								<td>Time</td>
								<td>:</td>
								<td>
									<t t-esc="shuttle.departure_time"/>
									<!-- - <t t-esc="shuttle.arrival_time"/>-->
								</td>
							</tr>
							<tr>
								<td>Driver</td>
								<td>:</td>
								<td><t t-esc="shuttle.assigned_driver_name"/></td>
							</tr>
							<tr>
								<td>Vehicle</td>
								<td>:</td>
								<td><t t-esc="shuttle.assigned_vehicle_name"/></td>
							</tr>
						</table>
						<hr style="border:solid 1px; border-color:darkgrey; margin:0px;"/>
					</t>
				</div>
			</t>
		</t>
	</t>
	

	<!-- LIST CONTRACT -->

	<t t-name="website_mobile_app_list_contract">
		<t class="full_height container universal_website_mobile_app">
			<t t-if="contract_datas.length == 0">
				<div class="col-xs-12" style="text-align: center; color: darkgray;">
					<h4>Data Kosong</h4>
				</div>
			</t>
			<t t-if="contract_datas.length != 0">
				<t t-set="i" t-value="0"/>
				<t t-foreach="contract_datas" t-as="contract">
					<div class="list_contract clickable contract_detail" t-attf-id_contract = "{{i}}">
						<div class="card_padding" style="pointer-events:none;">
							<t t-if="user_group == 'approver'">
								<t t-esc="contract.name"/>
							</t>
							<t t-if="user_group != 'approver'">
								<div class="list_title">
									<b><t t-esc="contract.name"/></b>-
									<t t-if="contract.state == 'active'">
										<span class="green_text">
											<t t-esc="contract.state_name"/>
										</span>
									</t>
									<t t-if="contract.state != 'active'">
										<span class="red_text">
											<t t-esc="contract.state_name"/>
										</span>
									</t>
								</div>
								<t t-esc="contract.start_date"/> / <t t-esc="contract.end_date"/><br></br>
								Service Type: <t t-esc="contract.service_type"/><br></br>
							</t>
						</div>
						<hr style="border:solid 1px; border-color:darkgrey; margin:0px;"/>
					</div>
					<t t-set="i" t-value="i+1"/>
				</t>
			</t>
		</t>
	</t>

	<!-- DETAIL CONTRACT HEADER-->
	<t t-name="website_mobile_app_detail_contract">
		<div class="row contract_detail" id="menu_detail_contract_container">
			<h3 class="contract_detail"><b><t t-esc="contract_name"/></b></h3>
			<table class="table table-hover">
				<tbody>
					<tr>
						<t t-if="user_group == 'pic'">
							<td class = "td_tab_order" id="website_mobile_app_menu_info_contract" style = "vertical-align:middle;border-right:1px solid; border-color:lightgray">
								Info
							</td>
						</t>
						<t t-if="contract_service_type == 'By Order'">
							<td class = "td_tab_order" id="website_mobile_app_menu_usage_control" style="vertical-align:middle;">
								Usage Control
							</td>
						</t>
						<t t-if="user_group == 'pic'">
							<t t-if="contract_service_type == 'By Order'">
								<td class = "td_tab_order" id="website_mobile_app_menu_quota_changes" style = "vertical-align:middle;border-left:1px solid; border-color:lightgray">
									Quota Changes
								</td>
							</t>
						</t>
						<t t-if="contract_service_type == 'Shuttle'" style = "border-left:1px solid; border-color:lightgray">
							<td class = "td_tab_order" id="website_mobile_app_menu_shuttle" style="vertical-align:middle;">
								Shuttle
							</td>
						</t>
					</tr>
				</tbody>
			</table>
		</div>

		<div class="row contract_detail" id="detail_contract_main_container"/>
	</t>

	<!-- DETAIL CONTRACT INFO-->
	<t t-name="website_mobile_app_detail_contract_info">
		<div class="contract_info no_padding">
			<table class="table">
				<tr>
					<td>Status</td>
					<td>:</td>
